msgid "Usage"
msgstr "Utilização"

#: bin/apt2sb:11
msgid "Installs packages from repositories and packages them into a module."
msgstr "Instala pacotes de repositórios e os empacota num módulo."

#: bin/apt2sb:13 bin/sb:27
msgid "Commands"
msgstr "Comandos"

#: bin/apt2sb:14
msgid "Install package(s)"
msgstr "Instalar pacote(s)"

#: bin/apt2sb:15
msgid "Upgrade installed packages"
msgstr "Atualizar pacotes instalados"

#: bin/apt2sb:17 bin/dir2sb:14 bin/sb2iso:23 bin/savechanges:17
#: bin/chroot2sb:13 bin/script2sb:13 bin/sb2iso:24
msgid "Options"
msgstr "Opções"

#: bin/apt2sb:18
msgid "Use NAME as the filename for the module instead of PACKAGE1"
msgstr "Usar NOME como nome de ficheiro para o módulo em vez de PACOTE1"

#: bin/apt2sb:19 bin/chroot2sb:16 bin/script2sb:18
msgid "Use LEVEL as the filter level"
msgstr "Usar NÍVEL como nível de filtro"

#: bin/apt2sb:20 bin/dir2sb:16 bin/savechanges:18 bin/chroot2sb:17
#: bin/script2sb:15
msgid "Compression type (zstd, gzip, lzo, xz). Default: zstd"
msgstr "Tipo de compressão (zstd, gzip, lzo, xz). Predefinição: zstd"

#: bin/apt2sb:21 bin/dir2sb:15 bin/savechanges:19 bin/chroot2sb:18
#: bin/script2sb:14
msgid "Bundle extension. Default: sb"
msgstr "Extensão do pacote. Predefinição: sb"

#: bin/apt2sb:22 bin/dir2sb:17 bin/sb:37 bin/sb2iso:26 bin/savechanges:20
#: bin/chroot2sb:19 bin/script2sb:20 bin/sb2iso:27 bin/sb2iso:30
